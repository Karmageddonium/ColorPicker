<Window x:Class="TestApp.Core.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestApp.Core" xmlns:core="clr-namespace:ColorPickerWPF.Core;assembly=ColorPickerWPF.Core"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <core:ColorPickRow HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Options="LoadCustomPalette"/>
            <core:ColorPickRect Width="30" Height="30" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Options="LoadCustomPalette"/>
        </StackPanel>
        
    </Grid>
</Window>
